{
  "name": "kingmakercharactereditor",
  "version": "1.1.0",
  "description": "Kingmaker Character Editor",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "angular-build": "ng build --prod",
    "angular-build-debug": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "build": "npm run angular-build && npm run electron",
    "build-debug": "npm run angular-build-debug && npm run electron",
    "dist-windows": "electron-packager ./ kingmakercharactereditor --icon=dist/assets/logo --prune=true --platform=win32 --out=build --overwrite",
    "dist-osx": "electron-packager ./ kingmakercharactereditor --icon=dist/assets/logo --prune=true --platform=darwin --out=build --overwrite",
    "dist-linux": "electron-packager ./ kingmakercharactereditor --icon=dist/assets/logo --prune=true --platform=linux --out=build --overwrite",
    "dist": "npm run angular-build && electron-packager ./ kingmakercharactereditor --icon=dist/assets/logo --prune=true --platform=all --out=build --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericfitzgerald/KingmakerCharacterEditor.git"
  },
  "author": "Eric Fitzgerald",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ericfitzgerald/KingmakerCharacterEditor/issues"
  },
  "homepage": "https://github.com/ericfitzgerald/KingmakerCharacterEditor#readme",
  "dependencies": {
    "@angular/animations": "7.1.1",
    "@angular/common": "7.1.1",
    "@angular/compiler": "7.1.1",
    "@angular/core": "7.1.1",
    "@angular/forms": "7.1.1",
    "@angular/http": "7.1.1",
    "@angular/platform-browser": "7.1.1",
    "@angular/platform-browser-dynamic": "7.1.1",
    "@angular/router": "7.1.1",
    "@ng-bootstrap/ng-bootstrap": "^4.0.0",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "node-zip": "^1.1.1",
    "rxjs": "^6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "7.1.0",
    "@angular/compiler-cli": "7.1.1",
    "@angular/language-service": "7.1.1",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "electron": "^3.0.10",
    "electron-packager": "^12.2.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.1",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "3.1.6"
  }
}
